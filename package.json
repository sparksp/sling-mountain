{
  "name": "sling-mountain",
  "version": "0.0.1",
  "description": "Scenarios for practising climbing rope work at home on #SlingMountain",
  "main": "src/index.ts",
  "repository": "https://github.com/sparksp/sling-mountain",
  "author": "Phill Sparks <me@phills.me.uk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sparksp/sling-mountain/issues"
  },
  "homepage": "https://github.com/sparksp/sling-mountain#readme",
  "scripts": {
    "postinstall": "elm-tooling install",
    "prebuild": "rimraf dist",
    "build": "parcel build src/index.html",
    "dev": "parcel src/index.html",
    "lint:format": "find src tests review/src -name *.elm ! -name Tailwind.elm | xargs elm-format --yes",
    "lint:review": "elm-review",
    "lint": "run-s lint:format lint:review ;",
    "test": "elm-test",
    "test:watch": "elm-test --watch",
    "all": "run-s build test lint ;"
  },
  "staticFiles": {
    "staticPath": "public",
    "watcherGlob": "**"
  },
  "devDependencies": {
    "elm-hot": "^1.1.6",
    "elm-review": "^2.4.6",
    "elm-test": "^0.19.1-revision6",
    "elm-tooling": "^1.3.0",
    "elm-types": "^0.0.2",
    "node-elm-compiler": "^5.0.5",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-purgecss": "^3.0.0",
    "parcel-plugin-static-files-copy": "^2.5.0",
    "postcss-elm-tailwind": "^0.10.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^1.9.6",
    "typescript": "^4.1.3"
  },
  "dependencies": {}
}
