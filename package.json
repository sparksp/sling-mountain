{
  "name": "sling-mountain",
  "version": "0.0.1",
  "description": "Scenario tracker for #SlingMountain",
  "main": "src/index.ts",
  "repository": "https://github.com/sparksp/sling-mountain",
  "author": "Phill Sparks <me@phills.me.uk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sparksp/sling-mountain/issues"
  },
  "homepage": "https://github.com/sparksp/sling-mountain#readme",
  "scripts": {
    "clean:cache": "rimraf .cache",
    "clean:dist": "rimraf dist",
    "clean:elm": "rimraf elm-stuff",
    "clean": "run-p clean:cache clean:dist clean:elm ;",
    "prebuild": "run-s clean ;",
    "build": "parcel build src/index.html --out-file index.html",
    "dev": "parcel src/index.html --out-file index.html",
    "lint:format": "find src tests review/src -name *.elm ! -name Tailwind.elm | xargs elm-format --validate",
    "lint:review": "elm-review",
    "lint:analyse": "elm-analyse",
    "lint": "run-s lint:format lint:review lint:analyse ;",
    "test": "elm-test",
    "test:watch": "elm-test --watch",
    "all": "run-s build test lint ;"
  },
  "devDependencies": {
    "elm": "^0.19.1-3",
    "elm-analyse": "^0.16.5",
    "elm-format": "^0.8.3",
    "elm-hot": "^1.1.4",
    "elm-review": "^2.0.2",
    "elm-test": "^0.19.1-revision2",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2",
    "tailwindcss": "^1.4.6",
    "typescript": "^3.9.2"
  }
}
