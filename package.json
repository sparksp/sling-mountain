{
  "name": "sling-mountain",
  "version": "0.0.1",
  "description": "Scenarios for practising climbing rope work at home on #SlingMountain",
  "main": "src/index.ts",
  "repository": "https://github.com/sparksp/sling-mountain",
  "author": "Phill Sparks <me@phills.me.uk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sparksp/sling-mountain/issues"
  },
  "homepage": "https://github.com/sparksp/sling-mountain#readme",
  "scripts": {
    "clean:cache": "rimraf .cache",
    "clean:dist": "rimraf dist",
    "clean:elm": "rimraf elm-stuff",
    "clean": "run-p clean:cache clean:dist clean:elm ;",
    "prebuild": "run-s clean ;",
    "build": "parcel build src/index.html",
    "dev": "parcel src/index.html",
    "lint:format": "find src tests review/src -name *.elm ! -name Tailwind.elm | xargs elm-format --yes",
    "lint:review": "elm-review",
    "lint": "run-s lint:format lint:review ;",
    "test": "elm-test",
    "test:watch": "elm-test --watch",
    "all": "run-s build test lint ;"
  },
  "staticFiles": {
    "staticPath": "public",
    "watcherGlob": "**"
  },
  "devDependencies": {
    "elm": "^0.19.1-3",
    "elm-format": "^0.8.4",
    "elm-hot": "^1.1.4",
    "elm-json": "^0.2.8",
    "elm-review": "^2.3.3",
    "elm-test": "^0.19.1-revision4",
    "elm-types": "^0.0.2",
    "node-elm-compiler": "^5.0.5",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-purgecss": "^2.1.3",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "postcss-elm-tailwind": "^0.10.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^1.4.6",
    "typescript": "^4.0.5"
  },
  "dependencies": {}
}
